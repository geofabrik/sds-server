<div id="expert_view">
<h2>Expert View</h2>
<% if @tags.length == 0 %>
   <div class="osm_shadow_no_tags">No Tags saved for this object.</div>
<% else %>
   <table class="osm_shadow">
   <tr>
      <th>Key</th>
      <th>Value</th>
   </tr>
   <% @tags.each do |t| %>
      <tr>
         <td><%= t.key %></td>
         <td><%= t.value %></td>
      </tr>
   <% end %>
   </table>
<% end %>
</div>
